#include "holberton.h"
/**
 * times_table - print table
 *
 * Return: On success return 0
 */
void times_table(void)
{
int m;
int n;
int l;

for (m = 0; m < 10; m++)
{
for (n = 0; n < 10; n++)
{

l = n * m;
_putchar(l + '0');

if (l > 9)
{
_putchar((l / 10) + '0');
_putchar((l % 10) + '0');
}
}
else
{
_putchar(l + '0');
}
}
}
